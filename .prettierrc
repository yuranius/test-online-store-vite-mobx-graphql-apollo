{
  "singleQuote": true,
  "printWidth": 150,
  "tabWidth": 2,
  "useTabs": true,
  "semi": false,
  "jsxSingleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": false,
  "bracketSameLine": false,
  "importOrder": [
    "^components/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}